<%= form_with url: "/parks/#{@park.id}/otters", method: :post, local: true do |form| %>
  <%= form.label :otter_name %>
  <%= form.text_field :otter_name %>
  <%= form.label :able_to_work %>
  <%= form.select :able_to_work, [['yes', true], ['no', false]] %>
  <%= form.label :otter_age  %>
  <%= form.number_field :otter_age %>
  <%= form.submit 'Create otter' %>

<% end %>
